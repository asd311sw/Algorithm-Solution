function solution(arr1, arr2) {
    var answer = []
    var len = arr1.length

    for(var i=0;i<len;i++){
        var res = []
        var len_ = arr1[i].length
        for(var j=0;j<len_;j++){
            res.push(arr1[i][j]+arr2[i][j])
        }

        answer.push(res)
    }

    return answer;
}
